m() { bash tools/md2html.sh "${@:-$(st -m *.md)}"; }
c() { 
    for i in "${@:-$(st -m *.md)}"; do
        i=${i/.md/.html}
        i=${i/README/index}
        test -r "$i" || i="html/$i"
        bash -c "nohup chromium-browser $i >/dev/null 2>&1 &"
    done
    echo
}
gptchat() {
    bash -c "xxdiff . ~/b/${PWD##*/} &
export QT_QPA_PLATFORM=xcb; ~/gpt4all/bin/chat" &
}
